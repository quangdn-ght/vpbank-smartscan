# Test Environment Configuration
# ==============================

# Override production settings for testing
NODE_ENV=test
LOG_LEVEL=error

# API Configuration for Testing
# Use your actual API key for integration tests
# DASHSCOPE_API_KEY=your_test_api_key_here
OPENAI_BASE_URL=https://dashscope-intl.aliyuncs.com/compatible-mode/v1
AI_MODEL=qwen-vl-max

# Test-specific settings
REQUEST_TIMEOUT_MS=30000
MAX_RETRIES=2
RETRY_DELAY_MS=500

# Test file settings
SAVE_RESPONSES=true
RESPONSE_DIR=./test/responses
MAX_TOKENS=1000
TEMPERATURE=0.1

# Debug settings for testing
DEBUG_MODE=false
